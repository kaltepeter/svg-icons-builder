{
  "name": "@angular-extensions/svg-icons-builder",
  "version": "0.0.0",
  "description": "A blank schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run copy:files",
    "build:watch": "tsc -p tsconfig.json --watch && npm run copy:files",
    "copy:files": "cp ./package.json dist/ && cp ./src/*.json dist/ && cp ./src/svg-icons-builder/*.json ./dist/svg-icons-builder/",
    "format:write": "prettier --write ./src/**/*.ts",
    "test": "npm run build && ts-node -P tsconfig.spec.json node_modules/jasmine/bin/jasmine src/**/*_spec.ts"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./collection.json",
  "builders": "./builders.json",
  "dependencies": {
    "@angular-devkit/core": "^10.1.6",
    "@angular-devkit/schematics": "^10.1.6",
    "typescript": "~4.0.2",
    "svg-to-ts": "^5.6.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1001.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/node": "^12.11.1",
    "husky": "^4.3.0",
    "jasmine": "^3.5.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "ts-node": "^9.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  }
}
